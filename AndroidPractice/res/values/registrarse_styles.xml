<?xml version="1.0" encoding="utf-8"?>
<resources>           
     <style name="stile_textviews">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:width">60dp</item>
    </style>
               
	<style name="stile_edittext">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
    </style>
    <style name="stile_button">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="stile_textviews_sin">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="stile_layouts">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:layout_alignParentLeft">true</item>
    </style>
    <style name="stile_layouts_add">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">65dp</item>
        <item name="android:layout_alignParentLeft">true</item>
        <item name="android:gravity">center_vertical</item>
        
    </style>
    <style name="titulo_index">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">50dp</item>
        <item name="android:textStyle">bold</item>
    </style>
</resources>